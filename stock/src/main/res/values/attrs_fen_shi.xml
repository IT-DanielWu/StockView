<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FenShiView">
        <!--价格线宽度-->
        <attr name="priceStrokeWidth" format="dimension|reference" />

        <!--心脏半径-->
        <attr name="heartRadius" format="dimension|reference" />

        <!--心脏直径-->
        <attr name="heartDiameter" format="dimension|reference" />

        <!--初始透明度-->
        <attr name="heartInitAlpha" format="integer" />

        <!--心率-->
        <attr name="heartBeatRate" format="integer" />

        <!--心跳动画时间-->
        <attr name="heartBeatFractionRate" format="integer" />

        <!--是否启用滑动-->
        <attr name="enabledSlide" format="boolean" />
    </declare-styleable>
</resources>