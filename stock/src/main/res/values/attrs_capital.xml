<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CapitalView">
        <!--左上角标题(用于标注左边价格坐标)-->
        <attr name="leftTitle" format="string" />
        <!--右上角标题(用于标注右边inFlow坐标)-->
        <attr name="rightTitle" format="string" />

        <!--价格曲线颜色-->
        <attr name="priceColor" format="color" />
        <!--总资金净流入曲线颜色-->
        <attr name="financeInFlowColor" format="color" />
        <!--主力净流入曲线颜色-->
        <attr name="mainInFlowColor" format="color" />
        <!--散户净流入曲线颜色-->
        <attr name="retailInFlowColor" format="color" />

        <!--文本颜色-->
        <attr name="textColor" format="color" />
        <!--背景颜色-->
        <attr name="bgColor" format="color" />
    </declare-styleable>
</resources>