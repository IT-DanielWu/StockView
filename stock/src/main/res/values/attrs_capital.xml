<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CapitalView">
        <!--净流入单位-->
        <attr name="cv_in_flow_unit" format="integer" />
        <!--价格保留的位数-->
        <attr name="cv_price_digits" format="integer"/>
        <!--净流入保留的位数-->
        <attr name="cv_in_flow_digits" format="integer"/>
        <!--左上角标题(用于标注左边价格坐标)-->
        <attr name="cv_left_title" format="string" />
        <!--右上角标题(用于标注右边inFlow坐标)-->
        <attr name="cv_right_title" format="string" />

        <!--价格曲线颜色-->
        <attr name="cv_price_color" format="color" />
        <!--总资金净流入曲线颜色-->
        <attr name="cv_finance_in_flow_color" format="color" />
        <!--主力净流入曲线颜色-->
        <attr name="cv_main_in_flow_color" format="color" />
        <!--散户净流入曲线颜色-->
        <attr name="cv_retail_in_flow_color" format="color" />

        <!--文本颜色-->
        <attr name="cv_text_color" format="color" />
        <!--背景颜色-->
        <attr name="cv_bg_color" format="color" />
        <!--竖线颜色-->
        <attr name="cv_column_line_color" format="color" />
        <!--横线颜色-->
        <attr name="cv_row_line_color" format="color" />
    </declare-styleable>
</resources>