<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ysn.com.stockview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ysn.com.stockview.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".page.FenShiActivity" />
        <activity android:name=".page.FiveDayFenShiActivity" />
        <activity android:name=".page.CapitalActivity" />
        <activity android:name=".page.MiniFenShiActivity" />
        <activity android:name=".page.ProfitLossActivity" />
    </application>

</manifest>